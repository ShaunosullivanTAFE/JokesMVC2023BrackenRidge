@model IEnumerable<JokesMVC2023.Models.Data.Joke>

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.JokeQuestion)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.JokeAnswer)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
                <tr>
                    <td>
                    @Html.DisplayFor(modelItem => item.JokeQuestion)
                    </td>
                    <td>
                    @Html.DisplayFor(modelItem => item.JokeAnswer)
                    </td>
                    <td class="d-flex justify-content-around">
                    <input type="button" class="btn btn-outline-success" value="Add" onclick="addJokeToList(@item.Id)" />
                       
                    </td>
                </tr>
        }
    </tbody>
</table>