@model IEnumerable<JokesMVC2023.Models.Data.Joke>

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.JokeQuestion)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.JokeAnswer)
            </th>
            <th style="min-width:20vw"></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
                <tr>
                    <td>
                    @Html.DisplayFor(modelItem => item.JokeQuestion)
                    </td>
                    <td>
                    @Html.DisplayFor(modelItem => item.JokeAnswer)
                    </td>
                    <td class="d-flex justify-content-around">
                    <input type="button" class="btn btn-outline-danger" value="Remove" data-itemid="@item.Id" />
                       
                    </td>
                </tr>
        }
    </tbody>
</table>